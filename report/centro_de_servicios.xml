<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <report
                id="report_servicios"
                model="centro_de_servicios"
                string="Catalogo de Centros de Servicios"
                name="mun_dic.centros_de_servicio_report"
                file="mun_dic.centros_de_servicio_report"
                report_type="qweb-pdf"/>

        <template id="centros_de_servicio_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Descripción</th>
      <th scope="col">Dirección</th>
      <th scope="col">Teléfono</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row"></th>
      <td>Mark</td>
      <td>Otto</td>

    </tr>
  </tbody>
</table>
                        </div>
                    </t>
                </t>
            </t>
        </template>

    </data>
</odoo>